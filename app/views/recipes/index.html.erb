<% if current_user %>
<%= react_component("Recipes", props: { current_user_last_name: current_user.last_name, current_user_name: current_user.first_name, current_user: current_user.email, current_user_id: current_user.id }, prerender: false) %>
<% else %>
<%= react_component("Recipes", props: { current_user: nil }, prerender: true) %>
<% end %>